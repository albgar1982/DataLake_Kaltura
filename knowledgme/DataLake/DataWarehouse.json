              "columnProperties": ["notNull", "nonEmpty"]
            }
          },
          "serietitle": {
            "type": "string",
            "validation": {
              "columnProperties": ["nonEmpty","notNull"]
            }
          },
          "season": {
            "type": "string",
            "validation": {
              "columnProperties": ["nonEmpty","notNull"]
            }
          },
          "episode": {
            "type": "string",
            "validation": {
              "columnProperties": ["nonEmpty","notNull"]
            }
          },
          "fulltitle": {
            "type": "string",
            "validation": {
              "columnProperties": [
                "notNull","notNull"
              ],
              "product": {
                "(.*[a-zA-Z0-9]\\W[S]\\-[0-9]+\\W[E]\\-[0-9]+\\W[a-zA-Z0-9].*)": [
                  "Episode",
                  "Podcast",
                  "Documental"
                ]
              }
            }
          },
          "genres": {
            "type": "string",
            "validation": {
              "columnProperties": ["nonEmpty","notNull"]
            }
          },
          "playbacktime": {
            "type": "long",
            "validation": {
              "columnProperties": [
                "notNull",
                "nonNegative",
                "formatMilliseconds"
              ]
            }
          },
          "origin": {
            "type": "string",
            "validation": {
              "columnProperties": ["notNull", "nonEmpty"],
              "pattern": "(^\\b[A-Z\\s]+\\b$)"
            }
          },
          "device": {
